<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >    
	<ImageView 
	    android:id="@+id/navigation_detail_top_show"
	    android:src="@drawable/nav_route_result_start_point"
	    android:layout_marginLeft="20dip"
	    android:layout_marginBottom="5dip"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"/>
	<ImageView 
	    android:src="@drawable/dash_line"
	    android:layout_marginLeft="23dip"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"/>
    <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:gravity="center_vertical"
	    android:layout_margin="10dip" >        
	    <ImageView 
	        android:id="@+id/navigation_detail_type"
	    	android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"/>
	    <TextView 
	        android:id="@+id/navigation_detail_name"
	    	android:layout_marginLeft="10dip"
	    	android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"/>
    </LinearLayout>
    <ImageView 
	    android:src="@drawable/dash_line"
	    android:layout_marginLeft="23dip"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"/>
	<ImageView 
	    android:id="@+id/navigation_detail_bottom_show"
	    android:src="@drawable/nav_route_result_end_point"
	    android:layout_marginTop="5dip"
	    android:layout_marginLeft="20dip"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"/>
</LinearLayout>
